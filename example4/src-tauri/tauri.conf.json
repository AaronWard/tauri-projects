{
  "build": {
    "devPath": "../src",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "example4",
    "version": "0.0.0"
  },
  "tauri": {
    "pattern": {
      "use": "isolation",
      "options": {
        "dir": "../src"
      }
    },
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "windows": [
      {
        "title": "example4",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.aw.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
