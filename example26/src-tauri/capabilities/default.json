{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "image:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "shell:allow-open",
    "process:allow-restart",
    "process:default",
    "process:allow-exit",
    "updater:default",
    "updater:allow-check",
    "updater:allow-download-and-install",
    "updater:allow-install",
    "updater:allow-download",
    "dialog:default"
  ]
}